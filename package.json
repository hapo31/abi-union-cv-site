{
  "name": "ak-cv-union",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build; pnpm run db:update; cp ./static-config.json.in ./.vercel/output/config.json",
    "start": "next start",
    "lint": "next lint",
    "dev:db:create:volume": "docker volume create --name ak-cv-union-mysql-volume",
    "dev:db:run": "dotenv -e .env ./bin/run_db.sh",
    "dev:dv:stop": "docker stop ak-cv-union-mysql",
    "dev:db:update": "node ./bin/import_csv.js",
    "dev:db:clear": "docker volume rm ak-cv-union-mysql-volume",
    "dev:docker:clear": ""
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.7",
    "@mui/system": "^5.11.8",
    "@next/font": "13.1.6",
    "@types/node": "18.11.19",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "csv-parse": "^5.3.5",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "fp-ts": "^2.13.1",
    "io-ts": "^2.2.20",
    "isomorphic-fetch": "^3.0.0",
    "jotai": "^2.0.0",
    "mysql": "^2.18.1",
    "mysql2": "^3.1.2",
    "next": "13.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.0",
    "react-use": "^17.4.0",
    "swr": "^2.0.3",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/mysql": "^2.15.21",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.0.0"
  }
}
